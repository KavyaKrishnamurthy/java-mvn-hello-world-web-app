pipeline {
      agent { label 'slave2' } 
    stages {
        stage('cloneproject') {
            steps {
                sh 'cd /home/slave2/'
				sh 'git clone https://github.com/KavyaKrishnamurthy/java-mvn-hello-world-web-app'
                sh 'pwd'
                sh 'cd /home/slave2/wokspace/DockermavenCICD'
            }
        }
        stage('Buildprocess') {
            steps {
                sh 'cd /home/slave2/wokspace/DockermavenCICD'
                sh 'sudo docker build -t mavenbuild /'home/slave2/wokspace/DockermavenCICD'
                sh 'sudo docker tag '
                sh 'sudo docker push '
            }
        }
        stage('deployprocess') {
            steps {
                sh 'cd /home/slave2/wokspace/DockermavenCICD'
                sh 'sudo run -d -p 8083:8080 mavenbuild:latest'
            
            }
        }
    }
}
