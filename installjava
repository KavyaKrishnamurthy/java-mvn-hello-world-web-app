pipeline {
agent any
   stages { 
    stage("Clean code") { 
      steps { 
      sh "git clone https://github.com/KavyaKrishnamurthy/java-mvn-hello-world-web-app"
      }
      }
      stage("build code"){
      steps{
      sh "sudo apt-get update"
         sh "sudo apt-get install default-jdk"
      }
    }
    }
    }
